{
        "version": "2.0.0",
        "command": "make",
        "tasks": [
            {
                "label": "clean",
                "command": "make",
                "args": [
                    "clean"
                ],
                "problemMatcher": []
            },
            {
                "label": "convbin_exe",
                "command": "make",
                "args": [],
                "problemMatcher": {
                    "owner": "c",
                    "fileLocation": [
                        "relative",
                        "${workspaceRoot}"
                    ],
                    "pattern": {
                        "regexp": "^(.*):(\\d+):(\\d+):\\s+(warning|error):\\s+(.*)$",
                        "file": 1,
                        "line": 2,
                        "column": 3,
                        "severity": 4,
                        "message": 5
                    }
                }
            }
        ]
        // "presentation": {
